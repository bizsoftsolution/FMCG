             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:FMCG.PL.frm.Report"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
