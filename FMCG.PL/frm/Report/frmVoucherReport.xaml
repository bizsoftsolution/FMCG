        <GroupBox x:Name="grbPayable" Header="Voucher Report" Background="White" >
            <Grid>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="50*"/>
                    <!--<ColumnDefinition Width="50*"/>-->
                </Grid.ColumnDefinitions>
                <GroupBox x:Name="grbSupplier"  Grid.Column="0">
                    <DataGrid x:Name="dgvPayableSupplier" />
                </GroupBox>
                <!--<GroupBox x:Name="grbCustomer" Header="Customoer" Grid.Column="1">
                   <DataGrid x:Name="dgvPayableCustomer"/>
               </GroupBox>-->
            </Grid>
        </GroupBox>
    </Grid>
</UserControl>
